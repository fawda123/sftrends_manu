  file = '',
  rowlabel = 'Analyte/Station',
  caption = cap.val,
  caption.loc = 'top',
  rgroup = vars,
  n.rgroup = rep(nrow(tab)/3, 3),
  cgroup = c('Annual'),
  n.cgroup = c(2),
  rowname = stats,
  label = 'tab:trndsann'
  )

