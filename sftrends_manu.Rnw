  geom_line() +
  theme_minimal() + 
  scale_colour_manual(values = inte$colv, name = 'Interior') +
  scale_shape_manual(values = inte$shps, name = 'Interior') +
  theme(legend.position = 'top')
inteleg <- g_legend(inteleg)

suisleg <- ggplot(suis, aes(x = pts, y = pts, colour = Site_Code, shape = Site_Code)) +
  geom_point() +
  geom_line() +
  theme_minimal() + 
  scale_colour_manual(values = suis$colv, name = 'Suisun') +
  scale_shape_manual(values = suis$shps, name = 'Suisun') +
  theme(legend.position = 'top')
suisleg <- g_legend(suisleg)

ylab1 <- expression(paste(NH [4] ^ '+', ' (mg ', L^-1, ')'))
ylab2 <- expression(paste(NO [2] ^ '-', ' / ', NO [3] ^ '2-', ' (mg ', L^-1, ')'))
ylab3 <- expression(paste('DIN (mg ', L^-1, ')'))

pA <- ggplot_gtable(ggplot_build(toplo$plts[[1]]))
pB<- ggplot_gtable(ggplot_build(toplo$plts[[2]]))
pC <- ggplot_gtable(ggplot_build(toplo$plts[[3]]))
maxWidth = unit.pmax(pA$widths[2:3], pB$widths[c(2:3)], pC$widths[c(2:3)])
pA$widths[2:3] <- maxWidth
pB$widths[2:3] <- maxWidth
pC$widths[2:3] <- maxWidth

grid.arrange(
  arrangeGrob(
    arrangeGrob(pA, left = textGrob(ylab1, rot = 90)),
    arrangeGrob(pC, left = textGrob(ylab3, rot = 90)), 
    ncol = 1
  ), 
