<<prddat, fig.cap = 'Model results (points) and flow-normalized predictions (lines) for ten stations grouped by nitrogen analyte and geographic location in the Delta region (locations in \\cref{fig:delt_map}).  Results are annually-averaged for each water year from October to September.', fig.height = 8, fig.width = 8>>=
data(mods)
data(delt_dat)

cols <-  brewer.pal(9, 'Blues')[c(4:9)] %>% 
  colorRampPalette(.)

stat_meta <- delt_dat %>% 
  dplyr::select(Site_Code, Location) %>% 
  unique %>% 
  mutate(
    Location = factor(Location, 
           levels = c('Delta', 'Middle', 'Suisun'), 
           labels = c('Peripheral', 'Interior', 'Suisun')
    )
  ) %>% 
  arrange(Location, Site_Code) %>% 
  mutate(
    colv = c(cols(4), cols(3), cols(3)), 
    pts = c(1:10), 
    shps = c(c(16, 17, 18, 15), c(16:18), c(16:18))
  )

toplo <- mods %>% 
  mutate(
    anns = map(data, annual_agg), 
    Location = factor(Location, 
                      levels = c('Delta', 'Middle', 'Suisun'), 
                      labels = c('Peripheral', 'Interior', 'Suisun')
                      )
  ) %>% 
  left_join(stat_meta, by = c('Site_Code', 'Location')) %>% 
  select(-data, -flovar) %>% 
  unnest %>% 
  group_by(resvar) %>% 
  nest %>% 
  mutate(
    plts = map(data, function(x){
      
      p <- ggplot(x, aes(x = date, y = bt_fits, group = Site_Code)) +
        geom_point(alpha = 0.9, col = x$colv, pch = x$shps) + 
        geom_line(aes(y = bt_norm), col = x$colv) +
        facet_wrap(~ Location, ncol = 1, scales = 'free_y') + 
        theme_minimal() + 
        theme(
          legend.position = 'none', 
          axis.title.x = element_blank(), 
          axis.title.y = element_blank(), 
          panel.border = element_rect(colour = 'black', fill = NA, size = 0.5), 
          strip.text.x = element_text(size = 10)
          ) 
      
      return(p)
      
    })
  )

peri <- stat_meta %>% 
  filter(Location == 'Peripheral')
inte <- stat_meta %>% 
  filter(Location == 'Interior') 
suis <- stat_meta %>% 
  filter(Location == 'Suisun')

perileg <- ggplot(peri, aes(x = pts, y = pts, colour = Site_Code, shape = Site_Code)) +
  geom_point() +
  geom_line() +
  theme_minimal() + 
  scale_colour_manual(values = peri$colv, name = 'Peripheral') +
  scale_shape_manual(values = peri$shps, name = 'Peripheral') +
  theme(legend.position = 'top')
