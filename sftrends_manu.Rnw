obsdir <- filter(trndsyr, ztest < 0.05) %>% 
  mutate(dirchg = ifelse(perchg < 0, 'down', 'up')) %>% 
  group_by(cat, resvar) %>% 
  summarise(
    up = sum(dirchg == 'up'), 
